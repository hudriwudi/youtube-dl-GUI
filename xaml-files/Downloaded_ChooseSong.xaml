<Window x:Class="youtube_dl_v2.Downloaded_ChooseSong"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:youtube_dl_v2"
        mc:Ignorable="d"
        Title="Choose Song" Height="300" Width="560" Loaded="Window_Loaded" SizeToContent="WidthAndHeight">
    <Grid>
        <TextBlock HorizontalAlignment="Center" Margin="0,10,0,0" Text="Several files with the same name have been found. Please choose the file to use." TextWrapping="Wrap" VerticalAlignment="Top" Width="351" FontSize="15" TextAlignment="Center"/>
        <ListBox x:Name="listBox" Margin="62,73,47,53"/>
        <Button x:Name="cmdChoose" Content="Choose" HorizontalAlignment="Left" Margin="429,238,0,0" VerticalAlignment="Top" Width="84" Height="36" FontSize="14" Click="CmdChoose_Click"/>
    </Grid>
</Window>
