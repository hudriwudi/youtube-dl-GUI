<Window x:Class="youtube_dl_v2.Downloaded_ChangeSongInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:youtube_dl_v2"
        mc:Ignorable="d"
        Title="Change Song Info" Height="375" Width="428" Loaded="Window_Loaded" SizeToContent="WidthAndHeight">
    <Grid>
        <Label Content="Change Song Info" FontSize="18" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Label Content="Artist" Margin="64,51,289,156" FontSize="14"/>
        <TextBox x:Name="txtArtist" HorizontalAlignment="Left" Margin="132,51,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="209" Height="27" TextAlignment="Center" FontSize="14" GotKeyboardFocus="txtBox_GotKeyboardFocus" LostMouseCapture="txtBox_LostMouseCapture" LostKeyboardFocus="txtBox_LostKeyboardFocus"/>
        <Label Content="Song" Margin="64,99,289,108" FontSize="14"/>
        <TextBox x:Name="txtSongname" HorizontalAlignment="Left" Margin="132,99,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="209" Height="27" TextAlignment="Center" FontSize="14" GotKeyboardFocus="txtBox_GotKeyboardFocus" LostMouseCapture="txtBox_LostMouseCapture" LostKeyboardFocus="txtBox_LostKeyboardFocus"/>
        <Label Content="Link" Margin="64,152,289,170" FontSize="14"/>
        <TextBox x:Name="txtLink"  HorizontalAlignment="Left" Margin="132,145,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="209" Height="41" TextAlignment="Center" FontSize="14" GotKeyboardFocus="txtBox_GotKeyboardFocus" LostMouseCapture="txtBox_LostMouseCapture" LostKeyboardFocus="txtBox_LostKeyboardFocus"/>
        <Button x:Name="cmdChange" Content="Change" Margin="300,313,10,10" BorderThickness="2" FontSize="15" Click="CmdChange_Click"/>
        <Button x:Name="cmdReset" Content="Reset" Margin="10,313,300,10" BorderThickness="2" FontSize="15" Click="CmdReset_Click"/>
        <Button x:Name="cmdCancel" Content="Cancel" Margin="155,313,155,10" BorderThickness="2" FontSize="15" Click="CmdCancel_Click"/>
        <Label Content="Album" Margin="64,207,289,115" FontSize="14"/>
        <TextBox x:Name="txtAlbum" HorizontalAlignment="Left" Margin="132,207,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="209" Height="27" TextAlignment="Center" FontSize="14" GotKeyboardFocus="txtBox_GotKeyboardFocus" LostMouseCapture="txtBox_LostMouseCapture" LostKeyboardFocus="txtBox_LostKeyboardFocus"/>
        <Label Content="Genres" Margin="64,257,289,70" FontSize="14"/>
        <TextBox x:Name="txtGenres_1" HorizontalAlignment="Left" Margin="132,254.75,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="64" Height="36.5" TextAlignment="Center" FontSize="14" GotKeyboardFocus="txtBox_GotKeyboardFocus" LostMouseCapture="txtBox_LostMouseCapture" LostKeyboardFocus="txtBox_LostKeyboardFocus"/>
        <TextBox x:Name="txtGenres_2" HorizontalAlignment="Left" Margin="204,254.75,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="64" Height="36.5" TextAlignment="Center" FontSize="14" GotKeyboardFocus="txtBox_GotKeyboardFocus" LostMouseCapture="txtBox_LostMouseCapture" LostKeyboardFocus="txtBox_LostKeyboardFocus" RenderTransformOrigin="0.5,0.532"/>
        <TextBox x:Name="txtGenres_3" HorizontalAlignment="Left" Margin="277,254.75,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="64" Height="36.5" TextAlignment="Center" FontSize="14" GotKeyboardFocus="txtBox_GotKeyboardFocus" LostMouseCapture="txtBox_LostMouseCapture" LostKeyboardFocus="txtBox_LostKeyboardFocus"/>
    </Grid>
</Window>
